  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^17.0.29",
    "prettier": "^2.8.1",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3"
  },
  "license": "MIT",
  "dependencies": {
    "enquirer": "^2.3.6",
    "execa": "^7.0.0",
    "rimraf": "^3.0.2"
  }
}
