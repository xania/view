{
  "name": "create-xania",
  "version": "0.1.80",
  "private": false,
  "author": "Ibrahim ben Salah",
  "main": "./dist/cli.cjs",
  "types": "./dist/cli.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/cli.ts --minify --format cjs --target node16",
    "clean": "npx rimraf dist",
