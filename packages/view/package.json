{
  "name": "xania",
  "version": "0.5.62",
  "license": "MIT",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "typings": "./lib/index.ts",
  "type": "module",
  "keywords": [
    "xania",
    "signals",
    "observable",
    "frp"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs",
      "types": "./lib/index.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-dev-runtime.js",
      "require": "./dist/jsx-dev-runtime.cjs"
    },
    "./router": {
      "import": "./dist/router.js",
      "require": "./dist/router.cjs",
      "types": "./router/index.ts"
    }
  },
  "private": false,
  "files": [
    "types",
    "dist",
    "lib",
    "jsx-runtime.d.ts",
    "jsx-dev-runtime.d.ts",
    "router.d.ts"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "npm run clean && npx vite --watch build",
    "build": "npx vite build && npm run tsup",
    "tsup": "tsup",
    "clean": "npx rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "tsc"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "author": "Ibrahim ben Salah",
  "devDependencies": {
    "sass": "^1.63.6",
    "tslib": "^2.6.0",
    "tsup": "^7.1.0",
    "typescript": "5.3.3",
    "vitest": "0.32.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xania/view.git"
  }
}
