{
  "name": "@xania/view",
  "version": "0.4.18",
  "license": "MIT",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "typings": "./lib/index.ts",
  "types": "",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx-runtime.js",
      "require": "./dist/jsx-runtime.cjs",
      "types": "./dist/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx-dev-runtime.js",
      "require": "./dist/jsx-dev-runtime.cjs",
      "types": "./dist/jsx-dev-runtime.d.ts"
    }
  },
  "private": false,
  "files": [
    "types",
    "dist",
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "npx vite --watch build",
    "build": "npm run clean && npx vite build && npm run && npm run tsup:jsx-runtime && npm run tsup:jsx-dev-runtime",
    "tsup:jsx-runtime": "tsup jsx-runtime.ts --format esm --dts jsx-runtime.ts --external @xania/view",
    "tsup:jsx-dev-runtime": "tsup jsx-dev-runtime.ts --format esm --dts jsx-dev-runtime.ts --external @xania/view",
    "clean": "npx rimraf dist",
    "prepublishOnly": "npm version patch && npm run build ",
    "lint": "tsc"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "author": "Ibrahim ben Salah",
  "devDependencies": {
    "rollup-plugin-dts": "5.2.0",
    "rollup-plugin-typescript": "1.0.1",
    "sass": "^1.57.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "rollup-plugin-ts": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xania/view.git"
  }
}
