{
  "include": ["lib", "jsx-runtime", "build.config.ts", "jsx-dev-runtime.ts"],
  "compilerOptions": {
    "outDir": "dist",
    "module": "ESNext",
    "lib": ["DOM", "ESNext"],
    "importHelpers": true,
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "target": "ES2015",
    "esModuleInterop": true,
    "typeRoots": ["types"]
    // "noEmit": true
  }
}
