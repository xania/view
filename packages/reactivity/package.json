{
  "name": "@xania/reactivity",
  "version": "1.0.0-alpha",
  "license": "MIT",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "typings": "./lib/index.ts",
  "type": "module",
  "keywords": [
    "xania",
    "state, signals and arrows",
    "jsx",
    "ui",
    "frp",
    "arrowized frp"
  ],
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.cjs"
    }
  },
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "npm run clean && npx vite --watch build",
    "build": "npx vite build && npm run tsup",
    "tsup": "tsup",
    "clean": "npx rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "tsc"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "author": "Ibrahim ben Salah",
  "devDependencies": {
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xania/view.git"
  }
