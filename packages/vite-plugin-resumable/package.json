{
  "name": "vite-plugin-resumable",
  "version": "0.1.9",
  "license": "MIT",
  "typings": "./lib/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./lib/index.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "private": false,
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build": "npm run build:plugin && npm run build:hydrate",
    "build:plugin": "tsup lib/index.ts --sourcemap",
    "build:hydrate": "npx vite build --ssr lib/hydrate/index.ts --outDir dist/hydrate/ --minify",
    "prepublishOnly": "npm run clean && npm version patch && npm run build",
    "clean": "npx rimraf dist"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "author": "Ibrahim ben Salah",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xania/ssr.git"
  },
  "devDependencies": {
    "@ampproject/remapping": "^2.2.1",
    "@types/estree": "^1.0.1",
    "@types/node": "^18.16.6",
    "acorn": "^8.8.2",
    "estree-walker": "^3.0.3",
    "kleur": "^4.1.5",
    "magic-string": "^0.29.0",
    "tsup": "^6.7.0"
  },
  "peepDependencies": {
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "4.1.3"
  },
  "dependencies": {
    "estree-walker": "^3.0.3"
  }
}
