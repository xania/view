{
  "name": "@xania/scaffold",
  "version": "0.0.10",
  "private": false,
  "author": "Ibrahim ben Salah",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --minify --sourcemap --format esm,cjs --dts",
    "clean": "npx rimraf dist",
    "dev": "tsup src/index.ts --watch --sourcemap --format esm,cjs --dts",
    "prepublishOnly": "npm run clean && npm version patch && npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^17.0.29",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.1",
    "typescript": "^4.6.3"
  },
  "license": "MIT",
  "dependencies": {
    "@types/degit": "^2.8.3",
    "create-create-app": "^7.3.0",
    "degit": "^2.8.4",
    "enquirer": "^2.3.6",
    "execa": "^7.0.0",
    "octokit": "^2.0.14"
  }
}
